<?xml version="1.0" encoding="UTF-8"?>

<configuration>
  <appender class="ch.qos.logback.core.ConsoleAppender" name="console">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p %-30c{0} - %m%n</pattern>
    </encoder>
  </appender>

  <root level="ERROR">
    <appender-ref ref="console"/>
  </root>
</configuration>
